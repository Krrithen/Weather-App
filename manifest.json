{
    "manifest_version": 3,
    "name": "Weather Extension",
    "version": "1.0",
    "description": "Get the latest weather updates.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "activeTab",
      "https://api.openweathermap.org/*"
    ]
  }
  